{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "linebreak-style": "off",
      "no-console": "off",
      "import/no-named-default": "warn", 
      "no-trailing-spaces": "warn"
    }
  }